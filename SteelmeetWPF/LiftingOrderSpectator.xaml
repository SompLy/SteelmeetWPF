<UserControl
    x:Class="SteelmeetWPF.LiftingOrderSpectator"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=mscorlib"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SteelmeetWPF"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Width="385"
    Height="800"
    mc:Ignorable="d">
    <UserControl.Resources>
        <System:Double x:Key="FontSize">34</System:Double>
    </UserControl.Resources>
    <Grid>
        <Border
            Background="{DynamicResource middleground}"
            BorderBrush="{DynamicResource middleground}"
            BorderThickness="4"
            CornerRadius="8,8,8,8">
            <Border.Effect>
                <DropShadowEffect
                    BlurRadius="6"
                    Opacity="1"
                    ShadowDepth="8"
                    Color="Gray" />
            </Border.Effect>
            <Grid Background="{DynamicResource middleground}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                    <RowDefinition Height="*" />
                </Grid.RowDefinitions>
                <TextBlock
                    Name="L0Tb"
                    Grid.Row="0"
                    Grid.Column="0"
                    Margin="0,-5,0,0"
                    FontFamily="Cascadia Mono SemiBold"
                    FontSize="{StaticResource FontSize}"
                    FontWeight="DemiBold"
                    Foreground="{DynamicResource fontColorDark}"
                    Text="Sara Sandberg" />
                <TextBlock
                    Name="L1Tb"
                    Grid.Row="1"
                    Margin="0,-5,0,0"
                    FontFamily="Cascadia Mono SemiBold"
                    FontSize="{StaticResource FontSize}"
                    FontWeight="DemiBold"
                    Foreground="{DynamicResource fontColorDark}"
                    Text="Nalle Puhsson" />
                <TextBlock
                    Name="L2Tb"
                    Grid.Row="2"
                    Margin="0,-5,0,0"
