<UserControl
    x:Class="SteelmeetWPF.LiftingOrderSpectator"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=mscorlib"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:SteelmeetWPF"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Width="385"
    Height="850"
    mc:Ignorable="d">
    <UserControl.Resources>
        <System:Double x:Key="FontSize">34</System:Double>
    </UserControl.Resources>
    <Grid>
        <Border
            Background="{DynamicResource background}"
            BorderBrush="{DynamicResource background}"
            BorderThickness="4"
            CornerRadius="8,8,8,8">
        </Border>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*" />
                <RowDefinition Height="15*" />
            </Grid.RowDefinitions>
            <Grid Margin="0,0,0,8">
                <Border
                    Background="{DynamicResource middleground}"
                    BorderBrush="{DynamicResource middleground}"
                    BorderThickness="4"
                    CornerRadius="8,8,8,8">
                </Border>
                <TextBlock
                x:Name="Header"
                Grid.Row="0"
                Grid.Column="0"
                Margin="0,-4,0,0"
                FontFamily="{DynamicResource fontFamily}"
                FontSize="{StaticResource FontSize}"
                FontWeight="Bold"
                Foreground="{DynamicResource fontColorDark}"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Text="{Binding Title, RelativeSource={RelativeSource AncestorType=UserControl}}" />
            </Grid>
            <Grid Grid.Row="1">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <TextBlock
                    Name="L0Tb"
                    Grid.Row="0"
                    Grid.Column="0"
                    Margin="0,-5,0,0"
                    FontFamily="{DynamicResource fontFamily}"
                    FontSize="{StaticResource FontSize}"
                    FontWeight="Bold"
                    Foreground="{DynamicResource fontColorLight}"
                    Text="Ostmannen" />
                    <TextBlock
                    Name="L1Tb"
                    Grid.Row="1"
                    Margin="0,-5,0,0"
                    FontFamily="{DynamicResource fontFamily}"
                    FontSize="{StaticResource FontSize}"
                    FontWeight="Bold"
                    Foreground="{DynamicResource fontColorLight}"
                    Text="Nalle Puhsson" />
                    <TextBlock
                    Name="L2Tb"
