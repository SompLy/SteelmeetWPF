<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="backgroundColor">#343434</Color>
    <Color x:Key="background2Color">8E8B82</Color>
    <Color x:Key="middlegroundColor">E9DCBE</Color>
    <Color x:Key="accentColor">#8E8B82</Color>

    <SolidColorBrush x:Key="background" Color="#343434" />
    <SolidColorBrush x:Key="background2" Color="#8E8B82" />
    <SolidColorBrush x:Key="middleground" Color="#E9DCBE" />
    <SolidColorBrush x:Key="accent" Color="#8E8B82" />
    <FontFamily x:Key="fontFamily">pack://application:,,,/Fonts/#Montserrat</FontFamily>
    <FontFamily x:Key="fontFamilyMono">pack://application:,,,/Fonts/#JetBrainsMono</FontFamily>
    <SolidColorBrush x:Key="fontColorLight" Color="White" />
    <SolidColorBrush x:Key="fontColorDark" Color="Black" />
</ResourceDictionary>